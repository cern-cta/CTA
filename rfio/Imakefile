	   FileName($(LIBMANDIR),rfio_closedir.$(LIBMANSUFFIX)) \
	   FileName($(LIBMANDIR),rfio_fchmod.$(LIBMANSUFFIX)) \
	   FileName($(LIBMANDIR),rfio_fclose.$(LIBMANSUFFIX)) \
