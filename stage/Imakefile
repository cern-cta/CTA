#if defined(_WIN32)
$(CDBANA): FORCE
	pushd ..\db & $(MAKE) Cdbana.exe & popd
#else
$(CDBANA): FORCE
	cd ../db ; $(MAKE) Cdbana
#endif
Cstage_db.c Cstage_db.h: $(CDBANA) Cstage_db.des
	$(CDBANA) -I$(INCLUDES) -c Cstage_db.c -h Cstage_db.h Cstage_db.des
#endif
#endif

