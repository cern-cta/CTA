cpdskdsk:	cpdskdsk.sh
	sed -e s!CPSHELL!$(CPSHELL)! cpdskdsk.sh | sed -e s!BINDIR!$(BIN)! > cpdskdsk
	chmod 755 cpdskdsk

NormalProgramTarget(stager_castor, \
