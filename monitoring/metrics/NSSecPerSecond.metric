# Numer of request completed per second (in the NameServer),
# broken down by instance

<metric>
  name: NSSecPerSecond
  window: 60
  conditions: MSG=="Processing complete" and "@CERN.CH" in Username and DAEMON=="nsd"
  groupbykeys: NONE
  data: CounterHz(COUNT)
  handle_unordered: time_threshold
  nbins: 1
</metric>
