# Avg, Min and Max of the FileAge field in the GCD for deleted files
# broken down by SvcClass


<metric>
  name: GCFileAgeLhcb 
  window: 900
  conditions: DAEMON=="gcd" and INSTANCE=="c2lhcb" and GcType=="Automatic"
  groupbykeys: SvcClass
  data: Avg(FileAge), Min(FileAge), Max(FileAge)
  handle_unordered: time_threshold
  nbins: 1
</metric>
