# Counts how many requests per second
# breaking down by CASTOR instance

<metric>
  name: UnablePerformRequest
  unit: Count of requests
  category: General
  window: 60
  conditions: MSG=="Unable to perform request, notifying user" and DAEMON=="stagerd"
  groupbykeys: INSTANCE
  data: Counter(COUNT)
  handle_unordered: time_threshold
  nbins: 1
</metric>
