# The rate of each request type in the daemon srmfed
# for lhcb

<metric>
  name: LHCBSRMFEDRequestType
  unit: Hz
  category: LHCb
  window: 60
  conditions: DAEMON=="srmfed" and INSTANCE=="c2lhcb"
  groupbykeys: RequestType
  data: CounterHz(COUNT)
  handle_unordered: time_threshold
  nbins: 1
</metric>
