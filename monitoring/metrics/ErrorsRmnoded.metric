# Count of error from rmnoded

<metric>
  name: ErrorsRmnoded
  unit: Count of error from rmnoded
  category: General
  window: 60
  conditions: LVL in ["Error"] and DAEMON in ["rmnoded"]
  groupbykeys: INSTANCE, HOSTNAME
  data: Counter(COUNT)
  handle_unordered: time_threshold
  nbins: 1
</metric>
