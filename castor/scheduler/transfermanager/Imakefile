COMM Makefile for the CASTOR2 transfermanager
COMM
COMM @author Castor Dev team, castor-dev@cern.ch

InstallNonExecFile(dispatcher.py,$(PYTHON_LIBDIR),644)
InstallNonExecFile(aborter.py,$(PYTHON_LIBDIR),644)
InstallNonExecFile(synchronizer.py,$(PYTHON_LIBDIR),644)
InstallNonExecFile(serverqueue.py,$(PYTHON_LIBDIR),644)
InstallNonExecFile(transfermanagerdlf.py,$(PYTHON_LIBDIR),644)

InstallTarget(transfermanagerd,755)
ADMMANPAGE(transfermanagerd)
LOGROTATE(castor-transfer-manager,install)
SYSCONFIG(transfermanagerd,install)
INITSCRIPT(transfermanagerd,install)
