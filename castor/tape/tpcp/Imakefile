DependsOnLibrary(castor,castorclient)
DependsOnLibrary(castor/tape/gatewayprotocol,castortapegatewayprotocol)
DependsOnLibrary(castor/tape/net,castortapenet)
DependsOnLibrary(castor/tape/utils,castortapeutils)
DependsOnLibrary(common,castorcommon)
DependsOnLibrary(dlf,castordlf)
DependsOnLibrary(rfio,castorrfio)
DependsOnLibrary(vdqm,castorvdqm)
DependsOnLibrary(vmgr,castorvmgr)

DUMPTPBIN_SRCS = \
  Action.cpp \
  DumpTpCommand.cpp \
  DumpTpMain.cpp \
  Helper.cpp \
  StreamHelper.cpp \
  StreamOperators.cpp \
  TapeFileSequenceParser.cpp \
  TapeFseqRange.cpp \
  TapeFseqRangeListSequence.cpp \
  TapeFseqRangeSequence.cpp \
  TpcpCommand.cpp
DUMPTPBIN_OBJS = $(DUMPTPBIN_SRCS:.cpp=.o)

READTPBIN_SRCS = \
  Action.cpp \
  Helper.cpp \
  ReadTpCommand.cpp \
  ReadTpMain.cpp \
  StreamHelper.cpp \
  StreamOperators.cpp \
  TapeFileSequenceParser.cpp \
  TapeFseqRange.cpp \
  TapeFseqRangeListSequence.cpp \
  TapeFseqRangeSequence.cpp \
  TpcpCommand.cpp
READTPBIN_OBJS = $(READTPBIN_SRCS:.cpp=.o)

WRITETPBIN_SRCS = \
  Action.cpp \
  Helper.cpp \
  StreamHelper.cpp \
  StreamOperators.cpp \
  TapeFileSequenceParser.cpp \
  TapeFseqRange.cpp \
  TapeFseqRangeListSequence.cpp \
  TapeFseqRangeSequence.cpp \
  TpcpCommand.cpp \
  WriteTpCommand.cpp \
  WriteTpMain.cpp
WRITETPBIN_OBJS = $(WRITETPBIN_SRCS:.cpp=.o)

NormalProgramTarget(dumptp,$(DUMPTPBIN_OBJS),,,750)
EXEMANPAGE(dumptp)

NormalProgramTarget(readtp,$(READTPBIN_OBJS),,,750)
EXEMANPAGE(readtp)

NormalProgramTarget(writetp,$(WRITETPBIN_OBJS),,,750)
EXEMANPAGE(writetp)
