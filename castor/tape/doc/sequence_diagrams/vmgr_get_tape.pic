.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the rtcpcld object
object(RC,"rtcpcld");

# Put horizontal space between rtcpcld and the vmgr objects
placeholder_object(FakeObject1);
placeholder_object(FakeObject2);

# Define the vmgr object
object(V,"vmgr");

step();

# Message sequences
active(RC);
message(RC,V,"connect");
active(V);
message(RC,V,"get tape with N free bytes from tape-pool T");
message(V,RC,"tape");
message(V,RC,"return code of success (0)");
message(RC,V,"close connection");
inactive(V);
inactive(RC);

step();

complete(V);
complete(RC);

.PE
