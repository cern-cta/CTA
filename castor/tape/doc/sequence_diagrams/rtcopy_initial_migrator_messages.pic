.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the migrator process
object(M,"migrator");

# Put horizontal space between the migrator and rtcpd child processes
placeholder_object(FakeObject1);

# Define the child rtcpd process
object(C,"rtcpd child");

step();

# Message sequences

active(C);
message(C,M,"create initial connection to worker");
active(M);

step();

message(M,C,"tape");
message(C,M,"ack");

step();

message(M,C,"first file");
message(C,M,"ack");

step();

message(M,C,"request future callback");
message(C,M,"ack");

step();

message(M,C,"end of work list");
message(C,M,"ack");
# End of getRequestList

step();

complete(M);
complete(C);

.PE
