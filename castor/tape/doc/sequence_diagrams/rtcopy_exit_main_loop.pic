.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the objects
object(W,"worker");

# Put a horizontal space between the worker and rtcpd child processes
placeholder_object(Fake_1);
placeholder_object(Fake_2);

object(C,"rtcpd child");

step();

# Message sequences

active(C);
active(W);

message(C,W,"request more work");

step();

message(W,C,"end of work list");
message(C,W,"ack");

step();

message(W,C,"ack of request for more work");

step();

message(C,W,"end of session for disk/tape IO thread");
message(W,C,"ack");

message(C,W,"end of session for disk/tape IO thread");
message(W,C,"ack");

message(C,W,"end of session for disk/tape IO thread");
message(W,C,"ack");

step();

message(W,C,"end of entire session");
message(C,W,"ack");

step();

message(W,C,"close initial connection made to worker");
message(W,C,"close disk/tape IO thread control-connection");
message(W,C,"close disk/tape IO thread control-connection");
message(W,C,"close disk/tape IO thread control-connection");

step();

inactive(W);
inactive(C);

step();

complete(C);
complete(W);

.PE

