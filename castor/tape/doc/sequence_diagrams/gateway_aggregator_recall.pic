.PS

copy "sequence.pic";

# Define the objects
object(G,"gateway");
object(V,"vdqm");
object(A,"aggregator");

step();

# Message sequences
active(G);
message(G,V,"connect");
active(V);
message(G,V,"volume request");
message(V,G,"ack");
message(V,G,"volReqID");
message(G,V,"ack");
message(G,V,"disconnect");
inactive(V);
inactive(G);

step();

active(V);
message(V,A,"connect");
active(A);
message(V,A,"ClientInfo - job");
message(A,V,"RcpJobReply");
message(A,V,"disconnect");
inactive(V);

step();

message(A,G,"connect");
active(G);
message(A,G,"VolumeRequest");
message(G,A,"Volume");
message(A,G,"disconnect");
inactive(G);

step();

message(A,G,"connect");
active(G);
message(A,G,"FileToRecallRequest");
message(G,A,"FileToRecall");
message(A,G,"disconnect");
inactive(G);

message(A,G,"connect");
active(G);
message(A,G,"FileRecalledNotication");
message(G,A,"NotificationAck");
message(A,G,"disconnect");
inactive(G);

message(A,G,"connect");
active(G);
message(A,G,"FileToRecallRequest");
message(G,A,"NoMoreFiles");
message(A,G,"disconnect");
inactive(G);

message(A,G,"connect");
active(G);
message(A,G,"EndNotication");
message(G,A,"NotificationAck");
message(A,G,"disconnect");
inactive(G);

inactive(A);

complete(G);
complete(V);
complete(A);

.PE

