.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the recaller process
object(R,"recaller");

# Put horizontal space between the recaller and rtcpd child processes
placeholder_object(FakeObject1);

# Define the child rtcpd process
object(C,"rtcpd child");

step();

# Message sequences

active(C);
message(C,R,"create initial connection to worker");
active(R);

# Start of getRequestList
message(R,C,"tape");
message(C,R,"ack");

step();

message(R,C,"request future callback");
message(C,R,"ack");

step();

message(R,C,"end of work list");
message(C,R,"ack");
# End of getRequestList

step();

complete(R);
complete(C);

.PE
