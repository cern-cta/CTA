	 $(EXPORTH)/rfio_errno.h $(EXPORTH)/rfio_lcastorfdt.h
#endif
#if BuildRtcopyClient
IRTCP_H = \
	 FileName($(DESTDIRCASTOR),rtcp.h) FileName($(DESTDIRCASTOR),rtcp_api.h) \
	 FileName($(DESTDIRCASTOR),rtcp_constants.h) FileName($(DESTDIRCASTOR),rtcp_server.h)
ERTCP_H = \
	 $(EXPORTH)/rtcp.h $(EXPORTH)/rtcp_api.h \
	 $(EXPORTH)/rtcp_constants.h $(EXPORTH)/rtcp_server.h
#endif
#if BuildStageClient
ISTAGE_H = \
	 FileName($(DESTDIRCASTOR),stage.h) FileName($(DESTDIRCASTOR),stage_api.h) FileName($(DESTDIRCASTOR),stage_constants.h) \
	 FileName($(DESTDIRCASTOR),stage_limits.h) \
