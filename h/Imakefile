#if BuildDbClient
IDB_H = \
	 FileName($(DESTDIRCASTOR),Cdb_api.h)
EDB_H = \
	 $(EXPORTH)/Cdb_api.h
#endif
#if BuildNameServerClient || BuildNameServerLibrary
INS_H = \
	 FileName($(DESTDIRCASTOR),Cns_api.h) FileName($(DESTDIRCASTOR),Cns_constants.h) FileName($(DESTDIRCASTOR),Cns.h) FileName($(DESTDIRCASTOR),Cns_server.h)
ENS_H = \
	 $(EXPORTH)/Cns_api.h $(EXPORTH)/Cns_constants.h $(EXPORTH)/Cns.h $(EXPORTH)/Cns_server.h
#endif
#if BuildRfioClient
IRFIO_H = \
	 FileName($(DESTDIRCASTOR),rfcntl.h) FileName($(DESTDIRCASTOR),rfio.h) \
	 FileName($(DESTDIRCASTOR),rfio_api.h) FileName($(DESTDIRCASTOR),rfio_constants.h) \
	 FileName($(DESTDIRCASTOR),rfio_errno.h) FileName($(DESTDIRCASTOR),rfio_lcastorfdt.h)
ERFIO_H = \
