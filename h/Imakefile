	 FileName($(DESTDIRCASTOR),stage_protocol.h) FileName($(DESTDIRCASTOR),stage_struct.h) FileName($(DESTDIRCASTOR),stage_server_struct.h) FileName($(DESTDIRCASTOR),stage_shift.h)
ESTAGE_H = \
	 $(EXPORTH)/stage_api.h $(EXPORTH)/stage.h $(EXPORTH)/stage_constants.h \
	 $(EXPORTH)/stage_limits.h \
	 $(EXPORTH)/stage_macros.h $(EXPORTH)/stage_messages.h \
	 $(EXPORTH)/stage_protocol.h $(EXPORTH)/stage_struct.h $(EXPORTH)/stage_server_struct.h $(EXPORTH)/stage_shift.h
#endif
#if BuildTapeClient
ITAPE_H = \
	 FileName($(DESTDIRCASTOR),Ctape_api.h) FileName($(DESTDIRCASTOR),Ctape_constants.h) FileName($(DESTDIRCASTOR),Ctape.h)
ETAPE_H = \
	 $(EXPORTH)/Ctape_api.h $(EXPORTH)/Ctape_constants.h $(EXPORTH)/Ctape.h
#endif
#if BuildVdqmClient
IVDQM_H = \
	 FileName($(DESTDIRCASTOR),vdqm.h) FileName($(DESTDIRCASTOR),vdqm_api.h) \
	 FileName($(DESTDIRCASTOR),vdqm_constants.h)
EVDQM_H = \
	 $(EXPORTH)/vdqm.h $(EXPORTH)/vdqm_api.h \
