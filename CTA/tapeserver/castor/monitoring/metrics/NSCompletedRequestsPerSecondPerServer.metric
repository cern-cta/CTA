# Number of request completed per second (in the NameServer),
# broken down by server

<metric>
  name: NSCompletedRequestsPerSecondPerServer
  unit: Hz
  category: Nameserver
  window: 60
  conditions: MSG=="Processing complete" and DAEMON=="nsd"
  groupbykeys: HOSTNAME
  data: CounterHz(COUNT)
  handle_unordered: time_threshold
  nbins: 1
</metric>
