              <UML:Parameter isSpecification="false" visibility="public" xmi.id="115569" value="" type="117241" name="castorFile" />
              <UML:Parameter isSpecification="false" visibility="public" xmi.id="115570" value="" type="117233" name="subreq" />
             </UML:BehavioralFeature.parameter>
            </UML:Operation>
