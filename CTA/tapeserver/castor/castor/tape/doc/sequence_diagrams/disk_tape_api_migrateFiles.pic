.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the disk database API object
object(DAPI,"disk API");

# Define the tape database API object
object(TAPI,"tape API");

step();

# Message sequences
active(DAPI);
message(DAPI,TAPI,"migrateFiles");
active(TAPI);
message(TAPI,TAPI,"store request");
inactive(TAPI);
inactive(DAPI);

step();

complete(TAPI);
complete(DAPI);

.PE
