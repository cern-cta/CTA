COMM Makefile for the DLF client library
COMM
COMM @author Castor Dev team, castor-dev@cern.ch

PREFIX(MANSUBDIR) = doc/man/

ClientDependsOnLibrary(common,castorcommon)

DLFLIB_OBJS = dlf_lib.o
ClientSharedLibraryTarget(castordlf,$(DLFLIB_OBJS),,)

LIBMANPAGE(dlf_init)
LIBMANPAGE(dlf_isinitialized)
LIBMANPAGE(dlf_regtext)
LIBMANPAGE(dlf_write)
LIBMANPAGE(dlf_shutdown)

include LOCAL_PATH/web/Makefile
