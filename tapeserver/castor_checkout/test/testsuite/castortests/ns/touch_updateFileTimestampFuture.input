umask 0022; nstouch <nsFile>; echo $?
# Touch the file but print the year so that it may be caught and saved in the output tag.
# Must be on the same line as vars are reset between lines
futureYear=`expr 10 + \`date +%Y\``; nstouch -t "$futureYear"01010101 <nsFile>; echo $?; echo $futureYear
nsls -l <nsFile> | sed "s/  */ /g"
nsls -lu <nsFile> | sed "s/  */ /g"
