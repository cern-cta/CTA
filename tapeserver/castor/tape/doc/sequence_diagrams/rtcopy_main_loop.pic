.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the worker process
object(W,"worker");

# Put horizontal space between the worker and rtcpd child processes
placeholder_object(FakeObject1);

# Define the child rtcpd process
object(C,"rtcpd child");

step();

# Message sequences

active(C);
active(W);

step();

message(C,W,"request more work");

step();

message(W,C,"file");
message(C,W,"ack");

step();

message(W,C,"request future callback");
message(C,W,"ack");

step();

message(W,C,"end of work list");
message(C,W,"ack");

step();

message(W,C,"ack of request for more work");

step();

complete(W);
complete(C);

.PE

