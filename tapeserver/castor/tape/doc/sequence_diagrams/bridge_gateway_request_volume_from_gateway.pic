.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the tapegatewayd process
object(G,"gateway");

# Define the tapebridged process
object(B,"bridge");

step();

# Message sequences
active(B);
message(B,G,"connect");
active(G);
message(B,G,"VolumeRequest");
message(G,B,"Volume");
message(B,G,"close connection");
inactive(G);

step();

inactive(B);

complete(B);
complete(G);

.PE
