.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the objects
object(W,"worker");

# Put a horizontal space between the worker and rtcpd child processes
placeholder_object(Fake_1);
placeholder_object(Fake_2);

object(C,"rtcpd child");

step();

# Message sequences

active(W);
active(C);

message(C,W,"positioned for file (tape request)");
message(W,C,"ack");

step();

message(C,W,"positioned for file (file request)");
message(W,C,"ack");

step();

message(C,W,"file transfered");
message(W,C,"ack");

step();

message(C,W,"flushed to tape");
message(W,C,"ack");

step();

inactive(W);
inactive(C);

complete(W);
complete(C);

.PE

