.PS

copy "sequence.pic";

# Put a vertical space at the top of the document
step();
step();

# Define the objects
object(W,"worker");

# Put a horizontal space between the worker and rtcpd child processes
placeholder_object(Fake_1);

object(C,"rtcpd child");

object(T,"taped");

step();

# Message sequences

active(C);
active(W);

message(W,C,"end of entire session");
message(C,W,"ack");
message(C,T,"release tape");
active(T);
message(T,C,"ack");
inactive(T);

step();

inactive(W);
inactive(C);

step();

complete(C);
complete(W);
complete(T);

.PE

