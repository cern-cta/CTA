NormalProgramTarget(tplogger_test,tplogger_test.Osuf,$(GLOBLIB),$(LIBS),root,bin,755)
NormalProgramTarget(Ctape_test1,Ctape_test1.Osuf,$(GLOBLIB),$(LIBS),root,bin,755)
