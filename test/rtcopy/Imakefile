COMM		Make CASTOR tests for "rtcopy" directory

INCLUDES = FileName(FileName(..,..),h)

all:
	@echo 'no test available yet for "rtcopy" directory'

clean:
	-@RemoveFiles(FilesToClean)
 
clobber:        clean
	-@RemoveFiles($(PROGS))
 
depend:
	makedepend -Y$(INCLUDES) *.c
 
Makefiles:
 
