# Test for bug #90548: Broken logic when restarting subrequests waiting on recalls
# and bug #87426: stager_qry may not show STAGEIN when it should
rfcp <localFileName> <tapeFileName>
status=`stager_qry -M <tapeFileName> | tail -1 | awk '{print $NF;}'`; while [[ $status == 'CANBEMIGR' ]]; do sleep 10; status=`stager_qry -M <tapeFileName> | tail -1 | awk '{print $NF;}'` ; done
stager_qry -M <tapeFileName>
stager_rm -M <tapeFileName>
stager_qry -M <tapeFileName>
stager_get -S <tapeServiceClass> -M <tapeFileName>
stager_get -S <diskOnlyServiceClass> -M <tapeFileName>
status=`stager_qry -M <tapeFileName> -S <diskOnlyServiceClass> | tail -1 | awk '{print $NF;}'`; while [[ $status == 'STAGEIN' ]]; do sleep 10; status=`stager_qry -M <tapeFileName> -S <diskOnlyServiceClass> | tail -1 | awk '{print $NF;}'` ; done
stager_qry -S <tapeServiceClass> -M <tapeFileName>
stager_qry -S <diskOnlyServiceClass> -M <tapeFileName>
