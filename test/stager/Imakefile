COMM		Make CASTOR tests for "stager" directory

#include <Project.tmpl>

INCLUDES = FileName(FileName(..,..),h)

all:
	@echo 'no test available yet for "stager" directory'

clean:
	-@RemoveFiles(FilesToClean)
 
clobber:        clean
	-@RemoveFiles($(PROGS))
 
#if _WIN32
depend:
	@echo Not supported on this platform
#else
depend:
	makedepend -Y$(INCLUDES) *.c 2> /dev/null
#endif

Makefiles:
 
