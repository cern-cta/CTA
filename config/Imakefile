COMM		Make config files

MakeDir($(CONFIGDIR),root,bin,0755)

InstallNonExecFile(castor.example,$(CONFIGDIR),root,bin,0644)
InstallNonExecFile(castor_compiler.example,$(CONFIGDIR),root,bin,0644)
InstallNonExecFile(castor_db.example,$(CONFIGDIR),root,bin,0644)
InstallNonExecFile(castor_paths.example,$(CONFIGDIR),root,bin,0644)
InstallNonExecFile(castor_env.example,$(CONFIGDIR),root,bin,0644)
