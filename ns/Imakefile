clean:
	-@RemoveFiles(FilesToClean)

clobber:        clean
	-@RemoveFiles($(NSLIBTARGET) $(CLIENT) $(PROGS_D) Cns_oracle_ifce.c Cns_oracle_ifce.lis tp*)

