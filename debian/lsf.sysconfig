# Global setup for LSF

RUN_LSF=yes

if [ -n "$LD_LIBRARY_PATH" ]; then
  LD_LIBRARY_PATH=${LD_LIBRARY_PATH}:/usr/local/lsf/lib
else
  LD_LIBRARY_PATH=/usr/local/lsf/lib
fi

# inherit db config
[ -r /etc/sysconfig/castor ] && . /etc/sysconfig/castor
