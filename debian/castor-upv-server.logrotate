/var/log/Cupvdaemon {
    compress
    daily
    missingok
    rotate 15
}
/var/spool/Cupv/log {
    compress
    daily
    missingok
    rotate 15
}
/var/spool/upv/log {
    compress
    daily
    missingok
    rotate 15
}
