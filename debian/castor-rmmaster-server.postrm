#! /bin/sh
#
# $Id: castor-rmmaster-server.postrm,v 1.10 2007/08/07 14:58:38 waldron Exp $
#
CASTOR_SERVICE=rmMasterDaemon
export CASTOR_SERVICE
CASTOR_SERVICE_SPOOL=/var/spool/rmmaster
export CASTOR_SERVICE_SPOOL
[ -x /usr/sbin/castor-service.postrm ] && /usr/sbin/castor-service.postrm ${1+"$@"}
exit $?
