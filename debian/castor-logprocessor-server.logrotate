/var/log/castor/logprocessord.log.*dlf-syslog-to-db {
    compress
    missingok
    rotate 120
    sharedscripts
    postrotate
        /bin/kill -HUP `cat /var/run/logprocessord.*pid 2> /dev/null` 2> /dev/null || true
    endscript
}

/var/log/dlf/syslog.*input {
    compress
    size 10M
    missingok
    rotate 5
    delaycompress
}
