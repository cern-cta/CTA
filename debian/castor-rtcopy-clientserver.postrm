#! /bin/sh
#
# $Id: castor-rtcopy-clientserver.postrm,v 1.7 2005/08/08 16:51:23 jdurand Exp $
#
CASTOR_SERVICE=rtcpclientd
export CASTOR_SERVICE
CASTOR_SERVICE_SPOOL=/var/spool/rtcpclientd
export CASTOR_SERVICE_SPOOL
[ -x /usr/sbin/castor-service.postrm ] && /usr/sbin/castor-service.postrm ${1+"$@"}
exit $?
