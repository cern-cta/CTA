if [ -d /var/spool/expert ]; then
    for src in `ls /var/spool/expert/log.*.gz 2>/dev/null`; do
        dest=`basename $src | sed 's/log/expertd.log/'`
        /bin/mv --reply=no $src /var/log/castor/$dest 2>/dev/null
    done
    /bin/mv --reply=no /var/spool/expert/log /var/log/castor/expertd.log 2>/dev/null
    /bin/rm -f /var/spool/expert/sqlnet.log
    /bin/rmdir /var/spool/expert 2>/dev/null
fi
