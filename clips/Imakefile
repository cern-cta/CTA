COMM 	Make clips programs.

#include <Project.tmpl>

SPOOL = PolicySpool

IPROGS_CLIPS = FileName($(SPOOL),replica.pl) FileName($(SPOOL),recaller.pl) FileName($(SPOOL),migrator.pl) FileName($(SPOOL),recallerRetryPolicy.pl) FileName($(SPOOL),migratorRetryPolicy.pl)

all:

install: $(SPOOL) $(IPROGS_CLIPS)

MakeDir($(SPOOL),root,bin,0755)
InstallNonExecFile(recaller.pl,$(SPOOL),root,bin,0644)
InstallNonExecFile(migrator.pl,$(SPOOL),root,bin,0644)
InstallNonExecFile(recallerRetryPolicy.pl,$(SPOOL),root,bin,0644)
InstallNonExecFile(migratorRetryPolicy.pl,$(SPOOL),root,bin,0644)
InstallNonExecFile(replica.pl,$(SPOOL),root,bin,0644)

install.man:

export:

exportman:

COMM###################### CLEANING RULES #######################

clean:
clobber:
depend:
Makefiles:
FORCE:

COMM DO NOT DELETE THIS LINE -- make  depend  depends  on  it.
